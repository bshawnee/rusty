	"version": "0.2.0",
	"configurations": [
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug executable 'pg_rusty_proxy'",
			"cargo": {
				"args": [
					"build",
					"--bin=pg_rusty_proxy",
					"--package=pg_rusty_proxy"
				],
				"filter": {
					"name": "pg_rusty_proxy",
					"kind": "bin"
				}
			},
			"args": ["-d", "127.0.0.1:5433", "-u", "127.0.0.1:5432"],
			"cwd": "${workspaceFolder}"
		}

	]
}